{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "stock-ai-predictor",
  "compatibility_date": "2024-01-01",
  "pages_build_output_dir": "./dist",
  "compatibility_flags": ["nodejs_compat"],
  
  // KV Namespace for caching (1 hour TTL)
  // Create with: npx wrangler kv:namespace create CACHE
  // For local dev: npx wrangler kv:namespace create CACHE --preview
  "kv_namespaces": [
    {
      "binding": "CACHE",
      "id": "placeholder-id",
      "preview_id": "placeholder-preview-id"
    }
  ]
  
  // Environment variables (set in Cloudflare Pages Dashboard):
  // - ML_API_URL: Google Cloud Run ML API endpoint (e.g., https://stock-ml-api-xxxxx-uc.a.run.app)
  // - ALPHA_VANTAGE_API_KEY: Alpha Vantage API key
  // - FINNHUB_API_KEY: Finnhub API key
  // - OPENAI_API_KEY: OpenAI API key
  // - FRED_API_KEY: FRED API key
}
